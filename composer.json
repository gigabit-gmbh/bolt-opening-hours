{
    "name": "gigabit/opening-hours",
    "description": "A plugin to manage opening hours of a e.g. store",
    "type": "bolt-extension",
    "keywords": [
        "opening times",
        "hours",
        "opening",
        "times",
        "opening hours"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Helmrich",
            "email": "thomas@gigabit.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Gigabit\\OpeningHours\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Gigabit\\OpeningHours\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Gigabit\\OpeningHours\\OpeningHoursExtension"
    }
}
